class Solution {
    public boolean squareIsWhite(String coordinates) {
        int a1[]={0,1,0,1,0,1,0,1}; 
        int a2[]={1,0,1,0,1,0,1,0};
        int a3[]={0,1,0,1,0,1,0,1}; 
        int a4[]={1,0,1,0,1,0,1,0};
        int a5[]={0,1,0,1,0,1,0,1}; 
        int a6[]={1,0,1,0,1,0,1,0};
        int a7[]={0,1,0,1,0,1,0,1}; 
        int a8[]={1,0,1,0,1,0,1,0};
        int k=0;
        int l = Integer.parseInt(coordinates.charAt(1)+"");
        if(coordinates.charAt(0)=='a'){
            k=a1[l-1];
        }
        else if(coordinates.charAt(0)=='b'){
            k=a2[l-1];
        }
        else if(coordinates.charAt(0)=='c'){
            k=a3[l-1];
        }
        else if(coordinates.charAt(0)=='d'){
            k=a4[l-1];
        }
        else if(coordinates.charAt(0)=='e'){
            k=a5[l-1];
        }
        else if(coordinates.charAt(0)=='f'){
            k=a6[l-1];
        }
        else if(coordinates.charAt(0)=='g'){
            k=a7[l-1];
        }
        else if(coordinates.charAt(0)=='h'){
            k=a8[l-1];
        }
        if(k==0){
           return false;
       }
       else{
           return true;
       }
    }
}