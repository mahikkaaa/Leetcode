import java.util.*;
class Solution {
    public void reverseString(char[] s) {
        int n = s.length;
        int j = 0;
        char[] reverse = new char[n];
        for(int i =n-1; i>=0; i--){
            reverse[j] = s[i];
            j++;
        }
        for(int i=0; i<n; i++){
            s[i] = reverse[i];
        }
    }
}