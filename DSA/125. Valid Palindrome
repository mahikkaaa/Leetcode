class Solution {
    public boolean isPalindrome(String s) {
        s=s.toLowerCase();
        s = s.replaceAll("[^a-z0-9]", "");
        int n = s.length();
        String reversedstring = "";
        for(int i =n-1; i>=0; i--){
            reversedstring +=s.charAt(i);
            
        }
        return s.equals(reversedstring);
}}