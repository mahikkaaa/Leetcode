1365. How many Numbers Are Smaller than the Current Number

class Solution{
    public int[] smallerNumbersThanCurrent(int[] nums){
        int count=0;
        int n = nums.length;
        int[] result= new int[n];
        for(int i=0;i<n;i++){
            count=0;
            for(int j=0; j<n;j++){
                if(nums[j]<nums[i]){
                    count++;
                }
            }
            result[i]=count;
        }
        return result;
    }
}